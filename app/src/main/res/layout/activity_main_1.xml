<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Space
        android:id="@+id/center"
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:layout_marginEnd="150dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/one"
        android:layout_width="194dp"
        android:layout_height="140dp"
        android:background="@mipmap/img_8"
        android:rotation="-40"
        android:translationZ="1dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircle="@+id/center"
        app:layout_constraintCircleAngle="42"
        app:layout_constraintCircleRadius="204dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/two"
        android:layout_width="194dp"
        android:layout_height="140dp"
        android:background="@mipmap/img_6"
        android:translationZ="2dp"
        android:rotation="-30"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircle="@+id/center"
        app:layout_constraintCircleAngle="54"
        app:layout_constraintCircleRadius="206dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/three"
        android:layout_width="194dp"
        android:layout_height="140dp"
        android:background="@mipmap/img_5"
        android:rotation="-20"
        android:translationZ="3dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircle="@+id/center"
        app:layout_constraintCircleAngle="66"
        app:layout_constraintCircleRadius="221dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/four"
        android:layout_width="189dp"
        android:layout_height="141dp"
        android:background="@mipmap/img_2"
        android:rotation="-10"
        android:translationZ="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircle="@+id/center"
        app:layout_constraintCircleAngle="78"
        app:layout_constraintCircleRadius="236dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/five"
        android:layout_width="194dp"
        android:layout_height="140dp"
        android:background="@mipmap/img_1"
        android:translationZ="5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircle="@+id/center"
        app:layout_constraintCircleAngle="90"
        app:layout_constraintCircleRadius="246dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/six"
        android:layout_width="194dp"
        android:layout_height="140dp"
        android:background="@mipmap/img_3"
        android:rotation="10"
        android:translationZ="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircle="@+id/center"
        app:layout_constraintCircleAngle="102"
        app:layout_constraintCircleRadius="236dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/seven"
        android:layout_width="194dp"
        android:layout_height="140dp"
        android:background="@mipmap/img_4"
        android:rotation="20"
        android:translationZ="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircle="@+id/center"
        app:layout_constraintCircleAngle="112"
        app:layout_constraintCircleRadius="221dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/eight"
        android:layout_width="194dp"
        android:layout_height="140dp"
        android:background="@mipmap/img_7"
        android:rotation="30"
        android:visibility="visible"
        android:translationZ="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircle="@+id/center"
        app:layout_constraintCircleAngle="126"
        app:layout_constraintCircleRadius="206dp"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/nine"
        android:layout_width="194dp"
        android:layout_height="140dp"
        android:background="@mipmap/img_9"
        android:rotation="40"
        android:translationZ="1dp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintCircle="@+id/center"
        app:layout_constraintCircleAngle="138"
        app:layout_constraintCircleRadius="204dp"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>